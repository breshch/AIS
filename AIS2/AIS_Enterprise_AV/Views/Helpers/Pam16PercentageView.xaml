<Window x:Class="AIS_Enterprise_AV.Views.Helpers.Pam16PercentageView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        WindowStartupLocation="CenterScreen"
        x:Name="Pam16PercentageWindow"
        Title="Проценты ПАМ-16" Height="170" Width="300">
    <Grid Margin="20, 30">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        
        <StackPanel Grid.Row="0">
            <TextBlock Text="Проценты ПАМ-16"/>
            <TextBox Text="{Binding Pam16Percentage}" Margin="0, 5, 0, 0"/>
        </StackPanel>
        <Button Grid.Row="1" Content="Изменить" Command="{Binding EditCommand}" CommandParameter="{Binding ElementName=Pam16PercentageWindow}"
                Margin="0, 10, 0, 0"/>
        
    </Grid>
</Window>
