<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <UsingTask TaskName="MagicTask" AssemblyFile="KindOfMagic.exe"/>
  <Target Name="AfterCompile" DependsOnTargets="CoreCompile">
    <MagicTask Assembly="@(IntermediateAssembly)" References="@(ReferencePath)" KeyFile="$(KeyOriginatorFile)"
	MagicAttribute="NotifyAttribute" NoMagicAttribute="StopNotifyAttribute" RaiseMethod="OnPropertyChanged"/>
  </Target>
</Project>